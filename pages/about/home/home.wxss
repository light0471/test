.UCenter-bg {
  background-image: url(https://image.weilanwl.com/color2.0/index.jpg);
  background-size: cover;
  height: 550rpx;
  display: flex;
  justify-content: center;
  padding-top: 200rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color:white;
  font-weight: 400;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg open-data {
  width: 200rpx;
  height: 200rpx;
  display: flex;
  justify-content: center
}

map,.mapBox{
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  width: 750rpx;
  height: 300rpx;
}